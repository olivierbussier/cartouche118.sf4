<h6 class="card-title">
    <img src="{{ asset('/ico/pencil-square.svg') }}" alt="edit"
         data-url="{{ url('editTel', { id: telephone.id }) }}"
         onclick="cEdit(this)">
    <img src="{{ asset('/ico/trash.svg') }}" alt="del"
         data-url="{{ url('delTel', { id: telephone.id }) }}"
         onclick="cDel(this)">
    {% if telephone.label %}
        {{ telephone.label }}
    {% else %}
        {{ telephone.nom }}
    {% endif %}
</h6>

<div class="card-text">
    <a href="tel:{{ telephone.telephone }}">{{ telephone.telephone }}</a>
</div>
<div class="card-text">&nbsp;</div>

